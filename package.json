{
  "name": "site",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "gray-matter": "^4.0.2",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^9.0.0",
    "jest": "^26.6.3",
    "next": "^10.1.2",
    "next-compose-plugins": "^2.2.1",
    "next-optimized-images": "^2.6.2",
    "product-sans-webfont": "^1.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-loading-skeleton": "^2.0.1",
    "react-markdown": "^4.3.1",
    "remark": "^13.0.0",
    "remark-html": "^13.0.1",
    "styled-components": "^5.1.0",
    "styled-theme": "^0.3.3",
    "typeface-spartan": "^1.1.4"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "predeploy": "yarn run build",
    "deploy": "firebase deploy --only hosting",
    "test": "jest --watchAll --coverage --env=jest-environment-jsdom-sixteen",
    "test:build": "jest --watchAll=false -u",
    "eject": "react-scripts eject",
    "lint": "eslint ./ --fix"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/react": "^10.4.0",
    "@testing-library/react-hooks": "^3.3.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.23.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.1.3",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "husky": "^4.2.5",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "prettier": "^2.0.5",
    "react-test-renderer": "^16.13.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && git add ."
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/serviceWorker.js",
      "!src/index.js"
    ]
  },
  "homepage": "https://abhisheknairofficial.com"
}
